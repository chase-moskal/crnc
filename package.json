{
	"name": "crnc",
	"version": "0.0.0-dev.20",
	"description": "currency conversion and functionality for the web",
	"license": "ISC",
	"type": "module",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"files": [
		"x",
		"s"
	],
	"scripts": {
		"prepare": "run-s clean && run-p compile importmap && run-s test",
		"clean": "rimraf x && mkdirp x",
		"compile": "tsc",
		"importmap": "importly --host=unpkg < package.json > x/importmap.json",
		"start": "serve",
		"test": "cynic node x/tests.test.js"
	},
	"devDependencies": {
		"cynic": "^0.1.0-dev.14",
		"importly": "0.1.0-dev.0",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"serve": "^11.3.2",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"commotion": "0.1.0-dev.0"
	},
	"keywords": [
		"money",
		"currency",
		"currency-exchange",
		"currency-conversion",
		"currency-converter",
		"exchange-rates"
	]
}
